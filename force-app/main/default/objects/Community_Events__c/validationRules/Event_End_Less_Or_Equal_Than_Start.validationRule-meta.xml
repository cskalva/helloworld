<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Event_End_Less_Or_Equal_Than_Start</fullName>
    <active>true</active>
    <description>Event_End_Less_Or_Equal_Than_Start</description>
    <errorConditionFormula>Start__c   &gt;=   End__c</errorConditionFormula>
    <errorMessage>The end date must be greater than the start date.</errorMessage>
</ValidationRule>
